{
  "name": "security-alert-issues-bot",
  "version": "1.0.0",
  "description": "[Probot] Sync GitHub Security Alerts and GitHub Issues.",
  "author": "azu <azuciao@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com//security-alert-issues-bot.git",
  "homepage": "https://github.com//security-alert-issues-bot",
  "bugs": "https://github.com//security-alert-issues-bot/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\""
  },
  "dependencies": {
    "@types/nock": "^9.3.0",
    "lodash": "^4.17.13",
    "nock": "^10.0.0",
    "probot": "^7.2.0",
    "probot-config": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.134",
    "@types/node": "^11.9.0",
    "husky": "^2.4.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.17.2",
    "prettier": "^1.18.2",
    "smee-client": "^1.0.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "prettier": {
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 4
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
